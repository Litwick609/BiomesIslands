<Defs>

 <ThingDef ParentName="AnimalThingBase">
    <defName>BiomesIslands_Gigantaconda</defName>
    <label>gigantaconda</label>
    <description>A huge predatory snake. Gigantacondas can be highly aggressive if provoked.</description>
    <statBases>
      <MoveSpeed>4.8</MoveSpeed>
      <MarketValue>420</MarketValue>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
    </statBases>
    <tools>
      <li>
        <label>fangs</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>20</power>
        <cooldownTime>2.3</cooldownTime>
        <linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>12</power>
        <cooldownTime>2</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>BiomesIslands_EggGigantacondaFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>22.0</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>
          <min>1</min>
          <max>2</max>
        </eggCountRange>
      </li>
    </comps>
    <race>
      <body>Snake</body>
      <predator>true</predator>
      <maxPreyBodySize>3</maxPreyBodySize>
      <baseHungerRate>0.25</baseHungerRate>
      <baseBodySize>2.5</baseBodySize>
      <baseHealthScale>2.5</baseHealthScale>
      <foodType>CarnivoreAnimal</foodType>
      <leatherDef>Leather_Lizard</leatherDef>
      <wildness>0.88</wildness>
      <manhunterOnTameFailChance>0.25</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0.35</manhunterOnDamageChance>
      <nameOnTameChance>1</nameOnTameChance>
      <lifeExpectancy>30</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.35</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.8</minAge>
          <soundWounded>Pawn_Cobra_Wounded</soundWounded>
          <soundDeath>Pawn_Cobra_Death</soundDeath>
          <soundCall>Pawn_Cobra_Call</soundCall>
          <soundAngry>Pawn_Cobra_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalExotic</li>
      <li>AnimalFighter</li>
    </tradeTags>
  </ThingDef>

