<Defs>

  <ThingDef ParentName="AnimalThingBase">
    <defName>BiomesIslands_SeaTurtle</defName>
    <label>sea turtle</label>
    <description>This heavily armored sea-dwelling reptile is known for its slow moving speed and surprisingly vicious bite. Because of its natural armor, it is tough to kill and can do serious damage during drawn-out melee fights.</description>
    <statBases>
      <MoveSpeed>1.0</MoveSpeed>
      <MarketValue>400</MarketValue>
      <ArmorRating_Blunt>0.35</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.50</ArmorRating_Sharp>
      <ComfyTemperatureMin>0</ComfyTemperatureMin>
      <ComfyTemperatureMax>50</ComfyTemperatureMax>
    </statBases>
    <tools>
      <li>
        <label>beak</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>11</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>TurtleBeakAttackTool</linkedBodyPartsGroup>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>4</power>
        <cooldownTime>2.6</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>BiomesIslands_EggSeaTurtleFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>1</eggFertilizationCountMax>
        <eggLayIntervalDays>30</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>
          <min>2</min>
          <max>5</max>
        </eggCountRange>
      </li>
    </comps>
    <race>
      <body>TurtleLike</body>
      <baseBodySize>1</baseBodySize>
      <baseHungerRate>0.33</baseHungerRate>
      <baseHealthScale>0.8</baseHealthScale>
      <foodType>OmnivoreRoughAnimal</foodType>
      <leatherDef>Leather_Lizard</leatherDef>
      <wildness>0.75</wildness>
      <lifeExpectancy>180</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.7</minAge>
          <soundWounded>Pawn_Tortoise_Wounded</soundWounded>
          <soundDeath>Pawn_Tortoise_Death</soundDeath>
          <soundCall>Pawn_Tortoise_Call</soundCall>
          <soundAngry>Pawn_Tortoise_Angry</soundAngry>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
    </race>
    <tradeTags>
      <li>AnimalUncommon</li>
    </tradeTags>
  </ThingDef>

