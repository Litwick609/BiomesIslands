<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <ThingDef ParentName="BiomesCore_SaltWaterAnimal"  ADogSaidBody="Tail">
        <defName>BiomesIslands_WhiteShark</defName>
        <label>great white shark</label>
        <description>A large species of mackarel shark. Feared for their power and sharp teeth, they live mainly in warm waters and prefer to hunt large prey.</description>
        <statBases>
            <MoveSpeed>0</MoveSpeed>
            <SwimSpeed>8</SwimSpeed>
            <MarketValue>1500</MarketValue>
            <MeatAmount>10</MeatAmount>
            <LeatherAmount>20</LeatherAmount>
        </statBases>
        <tools>
            <li>
                <label>teeth</label>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>4</power>
                <cooldownTime>0.5</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                            <def>Stun</def>
                            <amount>38</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
            </li>
        </tools>
        <race>
            <herdAnimal>false</herdAnimal>
            <predator>true</predator>
            <body>Shark</body>
            <baseHungerRate>0.7</baseHungerRate>
            <baseBodySize>2</baseBodySize>
            <baseHealthScale>3.0</baseHealthScale>
            <foodType>CarnivoreAnimal</foodType>
            <gestationPeriodDays>360</gestationPeriodDays>
            <packAnimal>false</packAnimal>
            <wildness>0.95</wildness>
            <nuzzleMtbHours>275</nuzzleMtbHours>
            <petness>0.01</petness>
            <lifeExpectancy>35</lifeExpectancy>
            <trainability>Simple</trainability>
            <manhunterOnTameFailChance>0.6</manhunterOnTameFailChance>
            <manhunterOnDamageChance>1</manhunterOnDamageChance>
            <meatLabel>fish meat</meatLabel>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.4</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>0.8</minAge>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
        </race>
    </ThingDef>

