                    <li>Bite</li>
                </capacities>
                <power>12</power>
                <surpriseAttack>
                    <extraMeleeDamages>
                        <li>
                        <def>Stun</def>
                        <amount>18</amount>
                        </li>
                    </extraMeleeDamages>
                </surpriseAttack>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
            </li>
        </tools>
		<comps>
		  <li Class="CompProperties_EggLayer">
			<eggFertilizedDef>BiomesIslands_EggTunaFertilized</eggFertilizedDef>
			<eggFertilizationCountMax>1</eggFertilizationCountMax>
