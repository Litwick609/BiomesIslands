<Defs>

 
  <!--================== Baitfish Eggs ==================-->
  
  <ThingDef ParentName="EggFertBase">
	<defName>EggIcyTetraFertilized</defName>
	<label>icy tetra egg (fert.)</label>
	<description>A fertilized icy tetra egg. If all goes well, it should hatch into a fish. It can be eaten raw, but it's much better cooked.</description>
	<graphicData>
		<color>(236,217,184)</color>
		<drawSize>1.1</drawSize>
	</graphicData>
	<statBases>
		<MarketValue>10.0</MarketValue>
	</statBases>
	<comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6.5</hatcherDaystoHatch>
        <hatcherPawn>BiomesIslands_IcyTetra</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
