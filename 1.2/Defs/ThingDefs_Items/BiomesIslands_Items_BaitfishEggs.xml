	<graphicData>
		<color>(230,220,220)</color>
		<drawSize>0.8</drawSize>
	</graphicData>
	<statBases>
		<MarketValue>10.0</MarketValue>
	</statBases>
	<comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>6.5</hatcherDaystoHatch>
        <hatcherPawn>BiomesIslands_LimepeelAngelfish</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
</Defs>
